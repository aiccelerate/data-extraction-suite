        {
          "name": "isElective",
          "description": "Whether surgery is elective",
          "valueExpr": {
            "fhirPath": "priority='EL'",
            "dataType": "boolean"
          }
        },
        {
          "name": "duration"
        },
        {
          "name": "location"
        }
      ]
    },
    {
