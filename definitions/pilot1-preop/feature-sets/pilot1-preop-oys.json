        {
          "name": "gender"
        },
        {
          "name": "age",
          "description": "Age of the patient at that time",
          "valueExpr": {
            "fhirPath": "utl:getPeriod(birthDate, %referenceTimePoint, 'years')",
            "dataType": "integer"
          }
        }
      ]
